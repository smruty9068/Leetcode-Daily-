class Solution {
    public int[] findErrorNums(int[] nums) {
        int n1=nums.length;
        HashMap<Integer, Integer> map= new HashMap<>();
        int dup=-1, miss=-1;
        for(int n:nums){
            map.put(n,map.getOrDefault(n,0)+1);
        }
        for(int i=1;i<=n1;i++){
            if(!map.containsKey(i)){
                miss=i;
            }
            if(map.getOrDefault(i,0)>1){
                dup=i;
            }
        }
    return new int[]{dup,miss};
    }
}
